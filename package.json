{
	"name": "bun-socket-scanner",
	"type": "module",
	"version": "0.1.0",
	"description": "A Bun security scanner that integrates with Socket.dev for package vulnerability detection",
	"author": "ryoppippi",
	"license": "MIT",
	"homepage": "https://github.com/ryoppippi/bun-socket-scanner#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ryoppippi/bun-socket-scanner.git"
	},
	"bugs": {
		"url": "https://github.com/ryoppippi/bun-socket-scanner/issues"
	},
	"keywords": [
		"bun",
		"security",
		"scanner",
		"socket.dev",
		"vulnerability",
		"supply-chain"
	],
	"exports": {
		".": "./src/index.ts",
		"./package.json": "./package.json"
	},
	"main": "src/index.ts",
	"module": "src/index.ts",
	"files": [
		"README.md",
		"src/"
	],
	"engines": {
		"bun": ">=1.0.0"
	},
	"scripts": {
		"test": "bun test",
		"typecheck": "tsgo --noEmit",
		"lint": "eslint --cache .",
		"format": "bun lint --fix .",
		"start": "bun run src/index.ts"
	},
	"dependencies": {
		"@socketsecurity/sdk": "^1.4.75"
	},
	"devDependencies": {
		"@ryoppippi/eslint-config": "^0.3.7",
		"@typescript/native-preview": "^7.0.0-dev.20250825.1",
		"bun-types": "^1.2.21",
		"eslint": "^9.34.0",
		"eslint-plugin-format": "^1.0.1",
		"gunshi": "^0.26.3"
	}
}
